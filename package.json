{
  "name": "vue-router",
  "version": "4.0.0-alpha.7",
  "main": "dist/vue-router.cjs.js",
  "browser": "dist/vue-router.esm.js",
  "unpkg": "dist/vue-router.global.js",
  "module": "dist/vue-router.esm-bundler.js",
  "typings": "dist/vue-router.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/*.js",
    "dist/vue-router.d.ts",
    "playground",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:dts": "api-extractor run --local --verbose",
    "dev": "webpack-dev-server --mode=development",
    "release": "bash scripts/release.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "build:playground": "webpack --env.prod",
    "build:e2e": "webpack --env.prod --config e2e/webpack.config.js",
    "dev:e2e": "webpack-dev-server --mode=development --config e2e/webpack.config.js",
    "lint": "prettier -c --parser typescript \"{src,__tests__,e2e}/**/*.[jt]s?(x)\"",
    "lint:fix": "yarn run lint --write",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "jest --coverage",
    "test": "yarn run test:types && yarn run test:unit",
    "test:e2e": "node e2e/runner.js -e chrome",
    "test:e2e:ci": "node e2e/runner.js -e firefox"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "vue": "^3.0.0-beta.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.7.18",
    "@microsoft/api-extractor": "^7.7.13",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/jest": "^25.1.3",
    "@types/jsdom": "^16.2.1",
    "@types/webpack": "^4.41.11",
    "@types/webpack-env": "^1.15.0",
    "@vue/compiler-sfc": "3.0.0-beta.2",
    "axios": "^0.19.2",
    "browserstack-local": "^1.4.5",
    "chalk": "^4.0.0",
    "chromedriver": "^81.0.0",
    "codecov": "^3.6.5",
    "conventional-changelog-cli": "^2.0.31",
    "css-loader": "^3.5.2",
    "dotenv": "^8.2.0",
    "faked-promise": "^2.2.2",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.4.0",
    "jest-mock-warn": "^1.0.1",
    "lint-staged": "^10.1.6",
    "nightwatch": "^1.3.4",
    "nightwatch-helpers": "^1.2.0",
    "prettier": "^1.19.1",
    "rollup": "^1.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "selenium-server": "^3.141.59",
    "serve-handler": "^6.1.2",
    "style-loader": "^1.1.4",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "vue": "^3.0.0-beta.2",
    "vue-loader": "^16.0.0-alpha.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yorkie": "^2.0.0"
  }
}
